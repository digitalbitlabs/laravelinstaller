{
    "name": "postboxcms/laravelinstaller",
    "description": "An installation wizard for Laravel apps",
    "license": "MIT",
    "version": "v1.0.1",
    "authors": [
        {
            "name": "Sanket Raut",
            "email": "sanket@digitalbit.in"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "facade/ignition-contracts": "^1.0",
        "filp/whoops": "^2.7.2",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Postbox\\LaravelInstaller\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Postbox\\LaravelInstaller\\LaravelInstallerProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
